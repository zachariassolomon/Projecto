<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!--
Licensee: 
License Type: Evaluation
-->
<hibernate-mapping>
  <class lazy="false" name="TrabAASI.Projeto" table="Projeto">
    <id column="ID" name="ID" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="Nome" lazy="false" length="255" name="nome" not-null="false" type="string"/>
    <property column="Descricao" lazy="false" length="255" name="descricao" not-null="false" type="string"/>
    <property column="Data_criacao" lazy="false" length="20" name="data_criacao" not-null="true" type="long"/>
    <property column="Data_fim" lazy="false" length="20" name="data_fim" not-null="true" type="long"/>
    <property column="Estado" lazy="false" length="255" name="estado" not-null="false" type="string"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Users" table="User_Projeto">
      <key column="ProjetoID" not-null="true"/>
      <many-to-many class="TrabAASI.User" column="UserID">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Tarefas">
      <key column="ProjetoID" not-null="true"/>
      <one-to-many class="TrabAASI.Tarefa"/>
    </set>
  </class>
</hibernate-mapping>
