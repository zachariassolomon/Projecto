<hibernate-mapping>
  <class lazy="false" name="TrabAASI.Tarefa" table="Tarefa">
    <id column="ID" name="ID" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <many-to-one access="field" cascade="lock" class="TrabAASI.Tarefa" column="TarefaID" lazy="proxy" name="tarefa_mae" not-null="true">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="TrabAASI.Projeto" column="ProjetoID" lazy="proxy" name="projeto" not-null="true">
		</many-to-one>
    <property column="Descricao" lazy="false" length="255" name="descricao" not-null="false" type="string"/>
    <property column="Estado" lazy="false" length="255" name="estado" not-null="false" type="string"/>
    <property column="Prioridade" lazy="false" length="10" name="prioridade" not-null="true" type="integer"/>
    <property column="Data_inicio" lazy="false" length="20" name="data_inicio" not-null="true" type="long"/>
    <property column="Data_fim" lazy="false" length="20" name="data_fim" not-null="true" type="long"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Subtarefas">
      <key column="TarefaID" not-null="true"/>
      <one-to-many class="TrabAASI.Tarefa"/>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Interacoes">
      <key column="TarefaID" not-null="true"/>
      <one-to-many class="TrabAASI.Interacao"/>
    </set>
  </class>
</hibernate-mapping>
