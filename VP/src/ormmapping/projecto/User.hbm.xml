<hibernate-mapping>
  <class lazy="false" name="TrabAASI.User" table="`User`">
    <id column="ID" name="ID" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="Username" lazy="false" length="255" name="username" not-null="false" type="string"/>
    <property column="Password" lazy="false" length="255" name="password" not-null="false" type="string"/>
    <property column="Email" lazy="false" length="255" name="email" not-null="false" type="string"/>
    <set cascade="save-update,lock" lazy="true" name="ORM_Projetos" table="User_Projeto">
      <key column="UserID" not-null="true"/>
      <many-to-many class="TrabAASI.Projeto" column="ProjetoID">
			</many-to-many>
    </set>
  </class>
</hibernate-mapping>
